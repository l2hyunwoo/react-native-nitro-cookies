{
  "name": "react-native-nitro-cookies-monorepo",
  "version": "0.1.0",
  "description": "Monorepo for react-native-nitro-cookies",
  "private": true,
  "scripts": {
    "example": "yarn workspace react-native-nitro-cookies-example",
    "package": "yarn workspace react-native-nitro-cookies",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "typecheck": "yarn workspace react-native-nitro-cookies typecheck",
    "test": "yarn workspace react-native-nitro-cookies test",
    "prepare": "yarn workspace react-native-nitro-cookies prepare",
    "nitrogen": "yarn workspace react-native-nitro-cookies nitrogen",
    "clean": "del-cli package/android/build example/android/build example/android/app/build example/ios/build package/lib",
    "release": "yarn workspace react-native-nitro-cookies release-it --only-version"
  },
  "workspaces": [
    "package",
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@evilmartians/lefthook": "^1.12.3",
    "@release-it/conventional-changelog": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
